{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env" : {
    "jest" : true,
  },
  "settings": {
    "import/core-modules": ["gatsby-helpers", "config"],
  },
  "rules": {
      "semi": 0,
      "one-var-declaration-per-line": 0,
      "one-var": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/forbid-prop-types": 0,
  }
}
