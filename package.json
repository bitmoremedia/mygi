    "preact-compat": "^3.13.1",
    "react": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-helmet": "^4.0.0",
    "sitemap": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "body-parser": "^1.16.1",
